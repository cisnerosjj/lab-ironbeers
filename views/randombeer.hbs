<div class="container">
     <h1>Beer</h1>
     <div class="row ">
          {{#each randomBeer}}
          {{> beerpartial this}}
          <hr width="1" size="500">
               <h4>Food Pairing</h4>
               <ul>
                    {{#each food_pairing}}
                    <li>{{this}}</li>
                    {{/each}}
               </ul>
               <p><strong>Brewer Tips:</strong> {{this.brewers_tips}}</p>
       
          {{/each}}
     </div>
</div>